    <creationDate>2015-04-28</creationDate>
    <author>Revive Adserver</author>
    <authorEmail>revive@revive-adserver.com</authorEmail>
    <authorUrl>http://www.revive-adserver.com</authorUrl>
    <license>GNU Gneral Public License v2</license>
    <description>Plugin that provides MaxMind GeoIP Geotargeting.</description>
    <version>1.3.3</version>
    <oxversion>3.2.0-beta-rc3</oxversion>
    <type>package</type>

    <install>
        <files>
            <file path="{PLUGINPATH}">openXMaxMindGeoIP.readme.txt</file>
        </files>

        <!-- list the groups that are contained in this plugin -->
